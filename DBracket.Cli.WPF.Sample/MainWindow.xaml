<cliControls:CliWindow x:Class="DBracket.Cli.WPF.Sample.MainWindow"
                       xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                       xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                       xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                       xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                       xmlns:local="clr-namespace:DBracket.Cli.WPF.Sample"
                       xmlns:cliControls="clr-namespace:DBracket.Cli.WPF.Controls;assembly=DBracket.Cli.WPF"
                       Style="{StaticResource QuickAcces.Window.Style}"
                       mc:Ignorable="d"
                       Title="MainWindow"
                       Height="450"
                       Width="800">
    
    <Grid>
        <StackPanel Margin="10,10,10,10">
            <TextBlock Text="Search"
                       Foreground="White"
                       FontSize="10" />
            <TextBox x:Name="CommandInput"
                     VerticalContentAlignment="Center"
                     Height="32"
                     FontSize="16" />
        </StackPanel>
    </Grid>
</cliControls:CliWindow>
